
@foreach (var lancamento in Model)
{
    <li>
        <figure>
            <a href="@Url.Action("Detalhes", "Conteudo", new { @IdDoConteudo = lancamento.IdDoConteudo })"><img src="@lancamento.Imagem" alt="@lancamento.Titulo"></a>
            <figcaption>
                <span>@lancamento.Tipo</span>
                <h3><a href="@Url.Action("Detalhes", "Conteudo", new { @IdDoConteudo = lancamento.IdDoConteudo })">@lancamento.Titulo</a></h3>
                <span data-avaliacao="@lancamento.Avaliacao" class="fa estrelas"></span>
                <span>A partir de @lancamento.CustoLabel</span>
            </figcaption>
        </figure>
    </li>
}   
