
@foreach (var maisDoFornecedor in Model)
{
    <li>
        <figure>
            <a href="@Url.Action("Detalhes", "Conteudo", new { @IdDoConteudo = maisDoFornecedor.IdDoConteudo })"><img src="@maisDoFornecedor.Imagem" alt="@maisDoFornecedor.Titulo"></a>
            <figcaption>
                <span class="default-color">Game (eguru)</span>
                <h3 ><a href="@Url.Action("Detalhes", "Conteudo", new { @IdDoConteudo = maisDoFornecedor.IdDoConteudo })">@maisDoFornecedor.Titulo</a></h3>
                @*<span class="estrelas av-@maisDoFornecedor.Avaliacao fa primary-color"></span>*@
                <span class="tertiary-color">A partir de <strong>@maisDoFornecedor.CustoLabel</strong></span>
            </figcaption>
        </figure>
    </li>
}